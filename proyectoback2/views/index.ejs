<!DOCTYPE html>
<html lang="en">
    <%- include("templates/head") %> <!-- aca estoy trayendo el template head -->
<body>
  
      <h4> Carrito</h4>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Imagen</th>
          </tr>
        </thead>
        <tbody>
            <% data.forEach(function(producto){  %>
          <tr>
            <th scope="row"><%= producto.id %></th>
            <td><%= producto.nombre %></td>
            <td><%= producto.precio %></td>
            <td> <img src="<%= producto.img %>" width="150"/></td>
          </tr>
          <% }) %>
        </tbody>
      </table>


</body>
</html>